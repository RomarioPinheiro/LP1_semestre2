
public class Conta {
	int num;
	Double saldo;
	
	public int getNum() {
		return num;
	}
	public void setNum(int num) {
		this.num = num;
	}
	public Double getSaldo() {
		return saldo;
	}
	public void setSaldo(Double saldo) {
		this.saldo = saldo;
	}
	public Conta(int num, Double saldo) {
		super();
		this.num = num;
		this.saldo = saldo;
	}
	@Override
	public String toString() {
		return "Dados da conta==> "+"n°:" + num + ", saldo=" + saldo + "]";
	}
}
